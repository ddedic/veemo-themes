{
    "name": "ddedic/veemo-themes",
    "description": "Veemo Themes - core package. Frontend / Backend theme management. Layouts, partials, assets. It works with veemo-modules package.",
    "keywords": [
        "veemo",
        "veemo-themes"
    ],
    "homepage": "https://github.com/ddedic/veemo-themes",
    "license": "MIT",
    "authors": [
        {
            "name": "Danijel Dedic",
            "email": "dedic.d@gmail.com",
            "homepage": "http://ddedic.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Veemo\\Themes\\": "src"
        },
        "classmap": [
            "src/Exceptions/Exceptions.php"
        ],
    },
    "autoload-dev": {
        "psr-4": {
            "Veemo\\Themes\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
